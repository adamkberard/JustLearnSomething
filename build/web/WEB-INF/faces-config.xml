<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
     
    <managed-bean>
        <managed-bean-name>util</managed-bean-name>
        <managed-bean-class>Util</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>login</managed-bean-name>
        <managed-bean-class>Login</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean> 
    <managed-bean>
        <managed-bean-name>misconception</managed-bean-name>
        <managed-bean-class>Misconception</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean> 
    <managed-bean>
        <managed-bean-name>register</managed-bean-name>
        <managed-bean-class>Register</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean> 
    <managed-bean>
        <managed-bean-name>teacher</managed-bean-name>
        <managed-bean-class>Teacher</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean> 
    <managed-bean>
        <managed-bean-name>validation</managed-bean-name>
        <managed-bean-class>Validation</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>  
    <managed-bean>
        <managed-bean-name>mainPage</managed-bean-name>
        <managed-bean-class>MainPage</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>question</managed-bean-name>
        <managed-bean-class>Question</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>answer</managed-bean-name>
        <managed-bean-class>Answer</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>quiz</managed-bean-name>
        <managed-bean-class>Quiz</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>student</managed-bean-name>
        <managed-bean-class>Student</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
      
    <navigation-rule>
        <from-view-id>
            /index.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/mainPage.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>register</from-outcome>
            <to-view-id>/register.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>
            /register.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>registered</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>
            /mainPage.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>createTeacher</from-outcome>
            <to-view-id>/teacherRegister.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>startRandQuiz</from-outcome>
            <to-view-id>/quiz.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>viewTeacherQuizzes</from-outcome>
            <to-view-id>/viewTeacherQuizList.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>createCustomQuiz</from-outcome>
            <to-view-id>/createQuiz.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>createQuestion</from-outcome>
            <to-view-id>/createQuestion.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>viewResults</from-outcome>
            <to-view-id>/quizList.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>addStudent</from-outcome>
            <to-view-id>/addStudent.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>viewStudents</from-outcome>
            <to-view-id>/viewStudents.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>startCustomQuiz</from-outcome>
            <to-view-id>/pickCustomQuiz.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>viewLifeStats</from-outcome>
            <to-view-id>/viewStats.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>selectStudent</from-outcome>
            <to-view-id>/studentSelect.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>
            /studentSelect.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/viewStats.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>
            /pickCustomQuiz.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/quiz.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>
            /addStudent.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>done</from-outcome>
            <to-view-id>/mainPage.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>
            /viewStudents.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/viewStats.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>
            /createQuiz.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>refresh</from-outcome>
            <to-view-id>/createQuiz.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/mainPage.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>
            /teacherRegister.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>registered</from-outcome>
            <to-view-id>/mainPage.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>
            /createQuestion.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>created</from-outcome>
            <to-view-id>/mainPage.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>
            /quiz.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>goToQuestion</from-outcome>
            <to-view-id>/question.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>submitted</from-outcome>
            <to-view-id>/quizView.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>
            /question.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>answered</from-outcome>
            <to-view-id>/quiz.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>
            /quizList.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>toQuizView</from-outcome>
            <to-view-id>/quizView.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>
            /quizView.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>viewMisconception</from-outcome>
            <to-view-id>/misconceptionView.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>done</from-outcome>
            <to-view-id>/mainPage.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
